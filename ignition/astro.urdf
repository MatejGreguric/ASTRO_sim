<robot name="astro">

    <link name="base_footprint"/>

    <joint name="base_link_joint" type="fixed">
        <parent link="base_footprint"/>
        <child  link="base_link"/>
        <origin xyz="0 0 0.0360"/>
    </joint>

    <link name="base_link"/>

    <joint name = "chasis_joint" type = "fixed">
        <parent link = "base_link"/>
        <child link = "chasis_link"/>
        <origin xyz = "0 0 0"/>
    </joint>

    <link name = "chasis_link">
        <visual>
            <origin rpy = "0 0 0" xyz = "0 0 0"/>
            <geometry>
                <box size="2.5 1.5 0.1" />
            </geometry>
            <material name="dark_gray"/>
        </visual>
        <collision>
            <origin rpy = "0 0 0" xyz = "0 0 0"/>
            <geometry>
                <box size="2.5 1.5 0.1" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz = "0.0125 0 0.0146" rpy = "0 0 0"/>
            <mass value = "4.03"/>
            <inertia ixx="0.04056665167" ixy="0.0" ixz="0.0" iyy="0.03789879167" iyz="0.0" izz="0.07361265167"/>
        </inertial>
    </link>

    <joint name="left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0 0.153 0" rpy="-${pi/2} 0 0"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="left_wheel">
        <visual>
            <geometry>
                <cylinder length="0.0160" radius="0.0360" />
            </geometry>
            <material name="gray"/>
        </visual>

        <collision>
            <geometry>
                <cylinder length="0.0160" radius="0.0360" />
            </geometry>
        </collision>
        <inertial>
            <origin xyz = "0 0 0" rpy = "0 0 0"/>
            <mass value = "0.07"/>
            <inertia ixx="0.0000241733" ixy="0.0" ixz="0.0" iyy="0.0000241733" iyz="0.0" izz="0.00004536"/>
        </inertial>
    </link>

    <joint name="right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0 -0.153 0" rpy="1.57 0 0"/>
        <axis xyz="0 0 -1"/>
    </joint>

    <link name="right_wheel">
        <visual>
            <geometry>
                <cylinder length="0.0160" radius="0.0360" />
            </geometry>
            <material name="gray"/>
        </visual>

        <collision>
            <geometry>
                <cylinder length="0.0160" radius="0.0360" />
            </geometry>
        </collision>

        <inertial>
            <origin xyz = "0 0 0" rpy = "0 0 0"/>
            <mass value = "0.07"/>
            <inertia ixx="0.0000241733" ixy="0.0" ixz="0.0" iyy="0.0000241733" iyz="0.0" izz="0.00004536"/>
        </inertial>

    </link>

    <joint name = "fl_caster_wheel_joint" type = "fixed">
        <parent link = "base_footprint"/>
        <child link = "fl_caster_wheel"/>
        <origin xyz="0.096 0.0965 0.0146" rpy="0 0 0"/>
    </joint>

    <link name="fl_caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.0146"/>
            </geometry>
            <material name="white"/>
        </visual>

        <collision>
            <geometry>
                <sphere radius="0.0146"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz = "0 0 0" rpy = "0 0 0"/>
            <mass value = "0.063"/>
            <inertia ixx="0.000005371632" ixy="0.0" ixz="0.0" iyy="0.000005371632" iyz="0.0" izz="0.000005371632"/>
        </inertial>
    </link>

    <joint name="fr_caster_wheel_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link ="fr_caster_wheel"/>
        <origin xyz="0.096 -0.0965 0.0146" rpy="0 0 0"/>
    </joint>

    <link name="fr_caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.0146"/>
            </geometry>
            <material name="white"/>
        </visual>

        <collision>
            <geometry>
                <sphere radius="0.0146"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz = "0 0 0" rpy = "0 0 0"/>
            <mass value = "0.063"/>
            <inertia ixx="0.000005371632" ixy="0.0" ixz="0.0" iyy="0.000005371632" iyz="0.0" izz="0.000005371632"/>
        </inertial>
    </link>

    <joint name = "bl_caster_wheel_joint" type = "fixed">
        <parent link = "base_footprint"/>
        <child link = "bl_caster_wheel"/>
        <origin xyz="-0.092 0.1265 0.0146" rpy="0 0 0"/>
    </joint>

    <link name="bl_caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.0146"/>
            </geometry>
            <material name="white"/>
        </visual>

        <collision>
            <geometry>
                <sphere radius="0.0146"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz = "0 0 0" rpy = "0 0 0"/>
            <mass value = "0.063"/>
            <inertia ixx="0.000005371632" ixy="0.0" ixz="0.0" iyy="0.000005371632" iyz="0.0" izz="0.000005371632"/>
        </inertial>
    </link>
    
    <joint name = "br_caster_wheel_joint" type = "fixed">
        <parent link = "base_footprint"/>
        <child link = "br_caster_wheel"/>
        <origin xyz="-0.092 -0.1265 0.0146" rpy="0 0 0"/>
    </joint>

    <link name="br_caster_wheel">
        <visual>
            <geometry>
                <sphere radius="0.0146"/>
            </geometry>
            <material name="white"/>
        </visual>

        <collision>
            <geometry>
                <sphere radius="0.0146"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz = "0 0 0" rpy = "0 0 0"/>
            <mass value = "0.063"/>
            <inertia ixx="0.000005371632" ixy="0.0" ixz="0.0" iyy="0.000005371632" iyz="0.0" izz="0.000005371632"/>
        </inertial>
    </link>

</robot>    
